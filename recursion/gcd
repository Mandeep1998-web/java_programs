public class HelloWorld{
public static int  gcd_using_recursion(int num1,int num2){
        if(num2!=0)
        return  gcd_using_recursion(num2,num2%num1);
        else
        return num1;
    }
     public static void main(String []args){
        int num1=100;
        int num2=10;
        int result=gcd_using_recursion(num1,num2);
        System.out.println(result);
     }
}
