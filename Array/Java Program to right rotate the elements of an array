//Java Program to right rotate the elements of an array
class Solution {
    public void rotate(int[] nums, int k) {
        int i,j;
        for(i=0;i<k;i++){
            int last=nums[nums.length-1];
            for(j=nums.length-1;j>0;j--){
                nums[j]=nums[j-1];   
            }
            nums[0]=last;
        }
        
    }
}
