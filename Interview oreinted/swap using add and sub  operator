// swap using bitwise operator
public class Gcd {
    public static void swapping(int num1,int num2){
   num1=num1+num2;
   num2=num1-num2;
   num1=num1-num2;
        System.out.println(num1);
        System.out.println(num2);
    }
  public static void main(String[] args){
      int num1=1;
      int num2=2;
     swapping(num1,num2);
  }
}
