//program to find lcm
package com.company;

public class Main {

    public static void main(String[] args) {
        int num1 = 5, num2 = 8;
        int max=num1>num2?num1:num2;
        do{
            if(max%num1==0 && max%num2==0){
                System.out.println(max);
                break;
            }
            max++;
        }
        while(true);
    }
}
