import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner obj=new Scanner(System.in);
      int n=obj.nextInt();
      int sum=0;
      int arr[]=new int[n];
      for(int i=0;i<n;i++){
          arr[i]=obj.nextInt();
      }
      int i,count=0,k,j;
      for( i=0;i<arr.length;i++){
          for(j=i;j<arr.length;j++){ 
              sum=0;
             for(k=i;k<=j;k++){
                     sum=sum+arr[k];
                    
             } 
              if(sum<0){ 
                        count++;
                    
                             }
    }
}
System.out.println(count);

    }}
