import java.util.*;
class Pyramid{
public static void main(String[] args){
	Scanner obj=new Scanner(System.in);
	int i,j ,num;
	System.out.println("Enter the number");
	 num=obj.nextInt();
	for(i=1;i<=5;i++){
	for(j=1;j<=9;j++){
    if(j>=6-i && j<=4+i){
     System.out.print("*");	
	}
	else
	System.out.print(" ");
	}
	System.out.println();
	
}
for(i=5;i>=1;i--){
	for(j=1;j<=9;j++){
		if(j>=6-i&&j<=4+i)
		System.out.print("*");
		else
		System.out.print(" ");
	}
	System.out.println();
}

}
}
