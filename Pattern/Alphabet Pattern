import java.util.*;
public class Alphabetic_Pattern{
	public static void main(String[] args){
		Scanner obj=new Scanner(System.in);
		int i;
		int j;
		System.out.print("Enter the number of rows");
		int num=obj.nextInt();
		int x=97-32;
		for(i=0;i<num;i++){
			for(j=0;j<=i;j++){
			System.out.print(" " + (char)(x+j));
			}
			System.out.println();
		}	
	}
}
